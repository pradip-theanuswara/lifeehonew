function usersearch_menu() {

	$items['searches'] = array(
	'title' => 'User Search form',
	'page callback' => 'drupal_get_form',
	'page arguments' => array('searches'),
	'access callback' => TRUE,
	'type' => MENU_NORMAL_ITEM
	);

	$items['usersearch/check_names'] = array(
	  'title' => 'Name Callback',
	  'page callback' => 'drupal_get_form',
